    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>


    <script>
 var x= @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ViewBag.us));
      var y=@Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ViewBag.ch));
  const data = {
    labels: x,
    datasets: [{
      label: 'My User Reservation',
      backgroundColor: 'rgb(0, 6, 6)',
      borderColor: 'rgb(51, 204, 204)',
      data: y ,
    }]
  };

  const config = {
    type: 'line',
    data: data,
    options: {}
  };
</script>








<div>
  <canvas id="myChart"></canvas>
</div>













<script>
  const myChart = new Chart(
    document.getElementById('myChart'),
    config
  );
  
</script>
